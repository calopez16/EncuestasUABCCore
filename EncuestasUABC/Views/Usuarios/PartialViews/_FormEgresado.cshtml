@model ApplicationUser;
@{
    Layout = null;
}

    <div class="row" id="div_Egresado">
        <input hidden asp-for="UsuarioEgresado.EgresadoId" />

        <div class="col-md-3 form-group">
            <label>Periodo de ingreso</label>
            <input asp-for="UsuarioEgresado.Egresado.PeriodoIngreso" class="form-control mask-periodo" type="text">
        </div>
        <div class="col-md-2 form-group">
            <label>Periodo de Egreso</label>
            <input asp-for="UsuarioEgresado.Egresado.PeriodoEgreso" class="form-control mask-periodo" type="text">
        </div>

        <div class="col-sm-3 form-group">
            <label>Correo</label>
            <input asp-for="UsuarioEgresado.Egresado.Correo" class="form-control" type="text">
        </div>
        <div class="col-md-3 form-group">
            <label>Correo Alterno</label>
            <input asp-for="UsuarioEgresado.Egresado.CorreoAlterno" class="form-control" type="text">
        </div>
        <div class="col-md-4 form-group">
            <label>Facebook</label>
            <input asp-for="UsuarioEgresado.Egresado.Facebook" class="form-control" type="text">

        </div>
        <div class="col-sm-3 form-group">
            <label>Otro</label>
            <input asp-for="UsuarioEgresado.Egresado.Otro" class="form-control" type="text">
        </div>
        <div class="col-sm-3 form-group">
            <label>Teléfono</label>
            <input asp-for="UsuarioEgresado.Egresado.Telefono" class="form-control mask-phone" type="text">
        </div>
        <div class="col-sm-3 form-group">
            <label>Celular</label>
            <input asp-for="UsuarioEgresado.Egresado.Celular" class="form-control mask-phone" type="text">
        </div>
    </div>
