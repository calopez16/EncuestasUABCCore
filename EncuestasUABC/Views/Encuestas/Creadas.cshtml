@using EncuestasUABC.Models;
@using EncuestasUABC.Utilidades;
@using EncuestasUABC.Enumerador;

@{
    ViewData["Title"] = "Encuestas Creadas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" src="~/lib/DataTables/datatables.min.js"></script>
<link href="~/lib/DataTables/datatables.min.css" rel="stylesheet" />
<div class="ibox p-4">
    <div class="ibox-head">
        <div class="ibox-title">
            <h1 class="page-title">@ViewData["Title"]</h1>
        </div>
        <button class="btn btn-primary" data-toggle="modal" data-target="#modal_CrearEncuesta">Agregar</button>
    </div>
    <div class="ibox-body">
        <table class="table table-hover" id="table_paginado">
            <thead>
                <tr>
                    <th>Fecha</th>
                    <th>Nombre</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>

<partial name="~/Views/Encuestas/PartialViews/_ModalCrearEncuesta.cshtml" />

<script type="text/javascript">
    var enum_EstatusEncuesta=@(EnumHelper.EnumToString<EstatusEncuestaId>())
</script>

<script src="~/js/Encuestas/Creadas.js"></script>


